{
  "name": "ipfs-postmsg-proxy",
  "version": "0.0.3",
  "description": "Proxy to an IPFS over window.postMessage",
  "main": "lib/index.js",
  "scripts": {
    "build": "run-p build:*",
    "build:js": "babel src --out-dir lib",
    "watch": "run-p watch:*",
    "watch:js": "babel src --watch --out-dir lib",
    "test": "run-s build lint test:*",
    "test:mocha": "mocha",
    "lint": "standard",
    "clean": "rm -rf lib/**",
    "prepublishOnly": "run-s build"
  },
  "author": "Alan Shaw",
  "license": "MIT",
  "dependencies": {
    "callbackify": "^1.1.0",
    "is-typedarray": "^1.0.0",
    "postmsg-rpc": "^2.1.1"
  },
  "devDependencies": {
    "async": "^2.6.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "hat": "0.0.3",
    "interface-ipfs-core": "^0.36.15",
    "ipfs": "^0.27.4",
    "ipfs-repo": "^0.18.5",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "realistic-structured-clone": "^1.0.1",
    "rimraf": "^2.6.2",
    "standard": "^10.0.3"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tableflip/ipfs-postmsg-proxy.git"
  },
  "keywords": [
    "IPFS",
    "postMessage",
    "window.postMessage",
    "proxy",
    "rpc"
  ],
  "bugs": {
    "url": "https://github.com/tableflip/ipfs-postmsg-proxy/issues"
  },
  "homepage": "https://github.com/tableflip/ipfs-postmsg-proxy#readme"
